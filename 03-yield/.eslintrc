{
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ]
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
